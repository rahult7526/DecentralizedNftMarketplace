{
  "name": "nft-marketplace-indexer",
  "version": "1.0.0",
  "scripts": {
    "indexer:start": "node scripts/service.js start",
    "indexer:stop": "node scripts/service.js stop", 
    "indexer:restart": "node scripts/service.js restart",
    "indexer:status": "node scripts/service.js status",
    "indexer:dev": "npx tsx scripts/indexer.ts",
