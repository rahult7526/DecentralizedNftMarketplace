  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "ethers": "^6.8.0",
    "swr": "^2.2.4"
  },
  "devDependencies": {
    "tsx": "^4.6.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}